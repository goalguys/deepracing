#!/bin/bash
echo $1/images $2 $3 "-R" $4 $5 $6 $7 "--optical_flow" "--json"
python3 imdir_to_lmdb.py $1/images $2 $3 "-R" $4 $5 $6 $7 "--optical_flow" "--json"
python3 generate_pose_sequence_labels.py $1 30 4.0 "--json"
python3 pose_sequence_labeldir_to_lmdb.py $1/images/pose_sequence_labels $1/images/pose_sequence_label_lmdb "--mapsize" 3e9
python3 generate_pose_sequence_key_file.py $1/images/pose_sequence_label_lmdb $1/goodkeys.txt
