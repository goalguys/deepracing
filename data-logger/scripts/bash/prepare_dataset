#!/bin/bash
dsroot=$1
python3 imdir_to_lmdb.py $dsroot/images $2 $3 --ROI $4 $5 $6 $7 --optical_flow --json --override
python3 generate_pose_sequence_labels.py $dsroot 30 4.0 --json
python3 pose_sequence_labeldir_to_lmdb.py $dsroot/images/pose_sequence_labels $dsroot/images/pose_sequence_label_lmdb --mapsize 3e9
python3 generate_pose_sequence_key_file.py $dsroot/images/pose_sequence_label_lmdb $dsroot/goodkeys.txt
